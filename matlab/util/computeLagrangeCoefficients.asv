function [lambda1, lambda2] = computeLagrangeCoefficients(H, a, B, c)
    N = size(H,1);
 
    if nargin < 4
        c = [1; -1];
    end
    if nargin < 3      
        B = zeros(2,N);
        oddId = [1:2:N];
        evenId = [2:2:N];
        B(1,oddId) = 1;
        B(2,evenId) = 1;
    end
    
end

